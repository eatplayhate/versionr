<link rel="stylesheet" href="/Content/plugins/dropzone-4.2.0/dropzone.css">
<script src="/Content/plugins/dropzone-4.2.0/dropzone.js"></script>

<script src="/Content/js/libs/emojify-1.1.0.min.js"></script>
<script src="/Content/js/libs/clipboard-1.5.9.min.js"></script>
*@
</html>